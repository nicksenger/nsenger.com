{
  "name": "senger-io",
  "version": "0.1.0",
  "scripts": {
    "build": "run-s build:server build:client copy:assets",
    "build:client": "webpack --mode production",
    "build:server": "bsb -make-world",
    "clean": "bsb -clean-world",
    "copy:assets": "cp -r ./assets/* ./src/client/dist/",
    "start": "node src/Server.bs.js",
    "test": "bsb -make-world && jest",
    "watch": "run-p watch:*",
    "watch:bs": "bsb -make-world -w",
    "watch:client": "webpack -w --mode development",
    "watch:server": "nodemon -r dotenv/config src/Server.bs.js"
  },
  "keywords": [
    "BuckleScript"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@piq9117/bs-mailgun": "^3.0.0-beta",
    "bs-express": "^0.12.1",
    "bs-fetch": "^0.5.0",
    "dotenv": "^8.1.0",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "reason-react": "^0.7.0",
    "wonka": "^3.2.1"
  },
  "devDependencies": {
    "@glennsl/bs-jest": "^0.4.9",
    "autoprefixer": "^9.6.1",
    "bs-dom-testing-library": "^0.5.1",
    "bs-jest-dom": "^2.0.1",
    "bs-platform": "^5.0.6",
    "bs-react-testing-library": "^0.6.0",
    "css-loader": "^3.2.0",
    "cssnano": "^4.1.10",
    "extract-loader": "^3.1.0",
    "file-loader": "^4.2.0",
    "fluorophore": "^1.1.1",
    "html-webpack-plugin": "^5.5.0",
    "nodemon": "^1.19.2",
    "npm-run-all": "^4.1.5",
    "postcss-loader": "^3.0.0",
    "sass-loader": "^8.0.0",
    "webpack": "^4.0.1",
    "webpack-cli": "^3.3.12"
  },
  "jest": {
    "testURL": "http://localhost/",
    "testEnvironment": "jsdom",
    "testRegex": "(/test/.*(Spec))\\.bs\\.js$"
  }
}
